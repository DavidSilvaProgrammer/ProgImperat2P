#include <stdio.h>
#include <stdlib.h>

int main() {
/*Declarações das variaveis*/

float A[5][5];int[20][20];
int i,j,k,x,y,z;float aux;char c;

/*Cálculo de A[i][j]*/
aux=0;
for(x=4*i;x<=4*i+3;x++){
    for(y=4*j;y<=*j+3;y++){
        aux+=B[x][y];
    }
    A[i][j]=aux/16;
}

/*Formação da matriz 20x20*/
for(i=k=0;i<20;i++){
    for(j=0;j<20;j++){
        B[i][j]=++k;
    }
}

printf("Escrita da matriz B");
for(i=k=0;i<20;i++){
    for(j=0;j<20;j++){
        printf("%4d",B[i][j]);
    }
}


    return 0;
}
