// Online C compiler to run C program online
#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int eh_primo(int x){
    int i, divisores=0;
    
    for(i=1;i<=x;i++){
        if(x % i == 0){
            divisores++;
        }
    }
    if(divisores==2){
        return 1; //é primo
    }else{
        return 0; //não é primo
    }
}
int main(){
    printf("Retorno: %d\n",eh_primo(11));
    
    return 0;
}
