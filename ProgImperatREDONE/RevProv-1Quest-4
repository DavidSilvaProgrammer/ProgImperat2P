/*4. Faça um programa que leia um número inteiro N (N > 0) e informe se o número é primo.*/

#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main(){
    int num,cont,result=0;
    printf("Digite um numero inteiro para saber se e primo:\n");
    scanf("%d",&num);
    for(cont=1;cont<=num;cont++){
        if(num%cont == 0){
            result++;
        }
    }
    if(result==2){
    printf("O numero %d e primo",num);
    }else{
        printf("O numero %d nao e primo",num);
    }
    
    return 0;
}
