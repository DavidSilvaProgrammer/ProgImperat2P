// OBS.: Laço FOR acaba depois do IF da soma de divisores. Se colocar isso até o final da função, da erro no código.

#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main() {
    int i,cont=1,loop=0,primo;

while(loop==0){
    int divisores=0;
    for (i=1;i<=cont;i++){
        if(cont%i==0){
            divisores++;
            primo=i;
        }
    }
    if(divisores==2){
        printf("%d, ",primo);
    }
    cont++;
}

    return 0;
}
