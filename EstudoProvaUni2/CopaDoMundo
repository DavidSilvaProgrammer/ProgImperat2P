// Online C compiler to run C program online
#include <stdio.h>

    typedef Struct{
        char nome[20];
        int prob;
    }Selecao;

int main() {
    Selecao selecao[32];
    printf("Digite o nome da seleção");
    gets(selecao[1].nome, 20, stdin);
    printf("%s",selecao[1].nome,);
    
    return 0;
}

