#include <stdio.h>
#include <stdlib.h>
#include <time.h>

typedef struct{
    char nome[20];
    int ponto, posicao_copa;
}Selecoes;

Selecoes fase_grupo(Selecoes x){
    
}


int main() {
    
    Selecoes grupos[4][8];
    Selecoes selecoes[32];
    
    int i=0,j,igual;
    
    srand(time(NULL));
    do{
        selecoes[i].posicao_copa=rand()%32;
        igual=0;
        for(j=0;j<i;j++){
            if(selecoes[j].posicao_copa==selecoes[i].posicao_copa){
                igual=1;
            }
        }
            if(igual==0){
                i++;
            }

            }while(i<32);
            
        for(i=0;i<32;i++){
            printf("%d ", selecoes[i].posicao_copa);
    
            
        }

    return 0;
}
