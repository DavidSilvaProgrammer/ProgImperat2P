#include <stdio.h>
#include <stdlib.h>

int main() {
    int matriz[3][3],i,j;
    
    printf("***********Entrada de Dados************");
    printf("\nDigite valor para os elementos da matriz: \n");
    for(i=0;i<3;i++){//i é índice da linha da matriz
        for(j=0;j<3;j++){//j é o indice da coluna da matriz
            printf("\nElemento[%d][%d] = ",i,j);
            scanf("%d", &matriz[i][j]);
        }
    }
    printf("\n\n***************************************\n\n");
    printf("\n\n************Saída de Dados*************\n\n");
    for(i=0;i<3;i++){//i é índice da linha da matriz
        for(j=0;j<3;j++){//j é o indice da coluna da matriz
            printf("\nElemento[%d][%d] = %d",i,j,matriz[i][j]);
        }
    }
    return 0;
}
