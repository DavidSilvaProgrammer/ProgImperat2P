/*2) Faça um programa que receba três números, calcule e mostre a multiplicação desses números*/

#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main() {
    int x,y,z,result;
    printf("Operacao de multiplicacao com tres numeros.\nDigite os tres numeros da operacao:\n");
    scanf("%d %d %d",&x,&y,&z);
    result=x*y*z;
    printf("\nResultado:\n%d * %d * %d = %d",x,y,z,result);
    

    return 0;
}
